# Frontend Environment Variables
# Copy this file to .env and update with your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_SOCKET_URL=http://localhost:3002

# Application Environment
REACT_APP_ENV=development

# WebRTC Configuration
REACT_APP_STUN_SERVER=stun:stun.l.google.com:19302
REACT_APP_TURN_SERVER=turn:your-turn-server.com:3478
REACT_APP_TURN_USERNAME=your-turn-username
REACT_APP_TURN_PASSWORD=your-turn-password

# Feature Flags
REACT_APP_ENABLE_RECORDING=true
REACT_APP_ENABLE_SCREEN_SHARE=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_BACKGROUND_BLUR=true

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_MIXPANEL_TOKEN=

# Error Tracking (optional)
REACT_APP_SENTRY_DSN=

# Application Branding
REACT_APP_APP_NAME=Nova Meet
REACT_APP_APP_DESCRIPTION=Modern Video Conferencing
REACT_APP_COMPANY_NAME=Nova Meet Team

# Social Media Links (optional)
REACT_APP_TWITTER_URL=https://twitter.com/novameet
REACT_APP_GITHUB_URL=https://github.com/thatamjad/Nova-Meet
REACT_APP_DISCORD_URL=

# Support
REACT_APP_SUPPORT_EMAIL=support@nova-meet.com
REACT_APP_SUPPORT_URL=https://nova-meet.com/support

# Development
GENERATE_SOURCEMAP=true
BROWSER=chrome
